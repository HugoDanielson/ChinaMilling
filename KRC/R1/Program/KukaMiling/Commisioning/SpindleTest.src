&ACCESS RVEO
DEF SpindleTest ( )
   DECL INT liRpm
   
   

   ;FOLD  . INI (IF $T1 OR $T2):
      CONTINUE
      IF ($T1 OR $T2)THEN
         GLOBAL INTERRUPT DECL 3 WHEN $STOPMESS==TRUE DO IR_STOPM ( )
         INTERRUPT ON 3
         BAS (#INITMOV,0)
      ENDIF
   ;ENDFOLD INI (IF $T1 OR $T2)
   
   ;Paramter Values
   liRpm= 200
   
   
   
   ;Routine Start
   Home()
   
   SpindleRun(liRpm,#CLOCKWISE)
   
   
   HALT
   
   SpindleStop( )
   
   HALT
  
   
END