&ACCESS RVO
&PARAM EDITMASK = *
DEFFCT BOOL GetName(sBaseName[] : IN,sName[] :OUT)
DECL CHAR sBaseName[]
DECL CHAR sName[]
DECL BOOL lbResult
DECL STATE_T lState
DECL INT lOffset

lbResult = STRCLEAR(sName[]);

lOffset = 0;
SWRITE(sName[],lState,lOffset,"%s%s%d%d",sBaseName[],"_",$DATE.HOUR,$DATE.MIN)
   RETURN TRUE
ENDFCT